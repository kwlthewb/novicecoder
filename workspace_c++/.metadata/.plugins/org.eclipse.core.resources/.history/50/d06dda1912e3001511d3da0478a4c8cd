class OpenFile {
public:
	OpenFile(std::string filename) { std::cout << filename << std::endl; }
	~OpenFile() { std::cout << "OpenFile is destroyed for " << filename << std::endl; }
};

int main() {
	OpenFile f("/some path/some file/some file2");
	OpenFile f2(f);
	return 0;
}
