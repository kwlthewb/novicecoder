#ifndef DOG_H_
#define DOG_H_
#include "Incls.h"

class dog {
	std::string _name;
public:
	dog(char* name) {
		_name = name;
		std::cout << _name << " dog is born." << std::endl;
		c.meow();
	}
	void bark() {
		std::cout << "Dog name is " << _name << std::endl;
	}
};



#endif /* DOG_H_ */
