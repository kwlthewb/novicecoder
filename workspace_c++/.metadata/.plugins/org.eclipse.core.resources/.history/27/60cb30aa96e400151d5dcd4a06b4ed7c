class cat {
	std::string _name;
public:
	cat(char* name) {
		_name = name;
		std::cout << _name << " cat is born." << std::endl;
	}
	void meow() {
		std::cout << "Cat name is " << _name << std::endl;
	}
};

cat c("Smokey");

