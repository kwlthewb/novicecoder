#ifndef CAT_H_
#define CAT_H_
#include "Incls.h"

class cat {
std::string _name;
public:
	cat(char* name) {
		_name = name;
		std::cout << _name << " cat is born." << std::endl;
		//Singleton::getDog()->bark();
	}
	~cat() { std::cout << _name << " is destroyed" << std::endl; }
	void meow() {
		std::cout << "Cat name is " << _name << std::endl;
	}

};

#endif /* CAT_H_ */
