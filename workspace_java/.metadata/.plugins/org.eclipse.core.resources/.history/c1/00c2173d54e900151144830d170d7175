
public class Airport {
	private String m_name;
	private String m_code;
public:		
	Airport(String n, String c) {
		
	}
	public static String findAirportCode(String toFind, Airport[] airports) {
		for(int i = 0; i < airports.length; i++) {
			if(airports[i].m_name.equals(toFind)) {
				return airports[i].m_code;
			}
		}
		return null;
	}
	
	public static void main(String[] args) {
		Airport[] aps = new Airport[5];
		aps[0].m_name = "Bejing";
		aps[0].m_code = "PEK";
		aps[1].m_name = "Hong Kong";
		aps[1].m_code = "HKG";
		aps[2].m_name = "San Francisco";
		aps[2].m_code = "SFO";
		aps[3].m_name = "Boston";
		aps[3].m_code = "BOS";
		aps[4].m_name = "London";
		aps[4].m_code = "LDN";
		System.out.println(findAirportCode("Beijing", aps));
		System.out.println(findAirportCode("London", aps));
		System.out.println(findAirportCode("Bei", aps));
	}
}
